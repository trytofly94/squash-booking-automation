{
  "name": "squash-booking-automation",
  "version": "1.0.0",
  "description": "Playwright-based squash court booking automation with intelligent slot selection and comprehensive testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:playwright": "playwright test",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test tests/e2e/",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "test:trace": "playwright show-trace",
    "test:merge-reports": "playwright merge-reports --reporter=html,line test-reports/blob-report",
    "test:last-run": "playwright show-report test-reports/html-report",
    "analyze:website": "node scripts/website-analysis.js",
    "codegen": "playwright codegen https://www.eversports.de/sb/sportcenter-kautz?sport=squash",
    "codegen:auth": "playwright codegen --save-storage=auth.json https://www.eversports.de/sb/sportcenter-kautz?sport=squash",
    "codegen:mobile": "playwright codegen --device='iPhone 13' https://www.eversports.de/sb/sportcenter-kautz?sport=squash",
    "inspect": "playwright test --debug",
    "test:profile": "playwright test --reporter=line,html --trace=on",
    "dry-run": "DRY_RUN=true npm run dev",
    "booking": "DRY_RUN=false npm run dev",
    "lint": "eslint src/ tests/ --ext .ts,.js",
    "lint:fix": "eslint src/ tests/ --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\"",
    "clean": "rm -rf dist/ test-artifacts/ test-reports/ screenshots/",
    "clean:logs": "rm -rf logs/*.log",
    "prestart": "npm run build",
    "type-check": "tsc --noEmit",
    "setup": "npm install && npx playwright install",
    "validate": "npm run type-check && npm run lint && npm run test:unit",
    "ci": "npm run validate && npm run test:playwright",
    "dev:ui": "node scripts/playwright-tools.js ui",
    "dev:debug": "node scripts/playwright-tools.js debug",
    "dev:codegen": "node scripts/playwright-tools.js codegen",
    "dev:analyze": "node scripts/playwright-tools.js analyze",
    "analyze": "npx ts-node scripts/live-dom-explorer.ts",
    "analyze:live": "npx ts-node scripts/live-dom-explorer.ts",
    "analyze:quick": "npx ts-node scripts/quick-selector-analysis.ts"
  },
  "keywords": [
    "automation",
    "playwright",
    "squash",
    "booking",
    "eversports",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.42.1",
    "dotenv": "^16.4.5",
    "playwright-extra": "^4.3.6",
    "playwright-extra-plugin-stealth": "^0.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
