{
  "name": "squash-booking-automation",
  "version": "1.0.0",
  "description": "Playwright-based squash court booking automation with intelligent slot selection and comprehensive testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:playwright": "playwright test",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/ --ext .ts,.js",
    "lint:fix": "eslint src/ tests/ --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\"",
    "clean": "rm -rf dist/",
    "prestart": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "automation",
    "playwright",
    "squash",
    "booking",
    "eversports",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.42.1",
    "dotenv": "^16.4.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}